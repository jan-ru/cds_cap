{
  "sap.ui5": {
    "rootView": {
      "viewName": "demo.ui5.view.App",
      "type": "XML",
      "async": true
    },
    "dependencies": {
      "minUI5Version": "1.100.0",
      "libs": {
        "sap.ui.table": {},
        "sap.m": {},
        "sap.tnt": {}
      }
    },
    "models": {
      "odata": {
        "type": "sap.ui.model.odata.v4.ODataModel",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "serviceUrl": "/analytics/"
        },
        "dataSource": "mainService"
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "demo.ui5.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": "TargetReports"
        }
      },
      "routes": [
        {
          "pattern": "",
          "name": "RouteReports",
          "target": "TargetReports"
        },
        {
          "pattern": "hierarchies",
          "name": "RouteHierarchies",
          "target": "TargetHierarchies"
        },
        {
          "pattern": "imports",
          "name": "RouteImports",
          "target": "TargetImports"
        }
      ],
      "targets": {
        "TargetReports": {
          "viewName": "Table",
          "viewId": "table"
        },
        "TargetHierarchies": {
          "viewName": "Hierarchies",
          "viewId": "hierarchies"
        },
        "TargetImports": {
          "viewName": "Imports",
          "viewId": "imports"
        }
      }
    }
  },
  "sap.app": {
    "id": "demo.ui5",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.2.4"
    },
    "dataSources": {
      "mainService": {
        "uri": "/analytics/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  }
}
