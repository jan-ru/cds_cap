<mvc:View
	controllerName="demo.ui5.controller.Financials"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns:m="sap.m"
	xmlns:t="sap.ui.table"
	height="100%"
>
