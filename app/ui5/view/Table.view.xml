<mvc:View
	controllerName="demo.ui5.view.Table"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns:m="sap.m"
	xmlns:tnt="sap.tnt"
	displayBlock="true"
	height="100%"
>
	<tnt:ToolPage id="toolPageTable">
		<tnt:sideContent>
			<tnt:SideNavigation
				expanded="false"
				itemSelect=".onItemSelect"
			>
				<tnt:NavigationList>
				</tnt:NavigationList>
				<tnt:fixedItem>
					<tnt:NavigationList>
						<tnt:NavigationListItem
							text="Version {versionInfo>/appVersion}"
							icon="sap-icon://information"
							key="version"
						/>
					</tnt:NavigationList>
				</tnt:fixedItem>
			</tnt:SideNavigation>
		</tnt:sideContent>
		<tnt:mainContents>
			<m:NavContainer
				id="pageContainerTable"
				initialPage="pageIncome"
				height="100%"
				width="100%"
			>
				<m:pages>
					<m:Page
						id="pageCashFlow"
						enableScrolling="false"
						showHeader="false"
					>
						<mvc:XMLView viewName="demo.ui5.view.CashFlow" async="true" />
					</m:Page>
					<m:Page
						id="pageCombined"
						enableScrolling="false"
						showHeader="false"
					>
						<mvc:XMLView viewName="demo.ui5.view.Financials" async="true">
							<mvc:customData>
								<core:CustomData
									key="fsType"
									value="COMBINED"
								/>
							</mvc:customData>
						</mvc:XMLView>
					</m:Page>
					<!-- Moved GL to Hierarchies -->
					<m:Page
						id="pagePivot"
						enableScrolling="false"
						showHeader="false"
					>
						<mvc:XMLView viewName="demo.ui5.view.Pivot" async="true" />
					</m:Page>
					<m:Page
						id="pageCreditors"
						enableScrolling="false"
						showHeader="false"
					>
						<core:Fragment
							fragmentName="demo.ui5.view.fragments.CreditorsTab"
							type="XML"
						/>
					</m:Page>
					<m:Page
						id="pageDebtors"
						enableScrolling="false"
						showHeader="false"
					>
						<core:Fragment
							fragmentName="demo.ui5.view.fragments.DebtorsTab"
							type="XML"
						/>
					</m:Page>
					<m:Page
						id="pageSales"
						enableScrolling="false"
						showHeader="false"
					>
						<mvc:XMLView viewName="demo.ui5.view.Sales" async="true" />
					</m:Page>
					<m:Page
						id="pageRevenueLTM"
						enableScrolling="false"
						showHeader="false"
					>
						<mvc:XMLView viewName="demo.ui5.view.RevenueLTM" async="true" />
					</m:Page>
					<m:Page
						id="pageCharts"
						enableScrolling="true"
						showHeader="false"
					>
						<mvc:XMLView viewName="demo.ui5.view.Charts" async="true" />
					</m:Page>
					<m:Page
						id="pageControls"
						enableScrolling="true"
						showHeader="false"
					>
						<mvc:XMLView viewName="demo.ui5.view.Controls" async="true" />
					</m:Page>
				</m:pages>
			</m:NavContainer>
		</tnt:mainContents>
	</tnt:ToolPage>
</mvc:View>
