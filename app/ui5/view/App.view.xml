<mvc:View
	controllerName="demo.ui5.view.App"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:tnt="sap.tnt"
	xmlns="sap.m"
	displayBlock="true"
	height="100%"
>
	<VBox height="100%">
		<tnt:ToolHeader>
			<Button
				icon="sap-icon://menu2"
				type="Transparent"
				press=".onSideNavButtonPress"
				tooltip="Expand/Collapse"
			/>
			<Button
				text="Reports"
				icon="sap-icon://bar-chart"
				type="Transparent"
				press=".onNavToReports"
				id="btnReports"
			/>
			<ToolbarSpacer />
		</tnt:ToolHeader>
		<App id="app" />
	</VBox>
</mvc:View>
