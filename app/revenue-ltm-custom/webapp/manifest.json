{
  "sap.ui5": {
    "rootView": {
      "viewName": "revenueltmcustom.view.Main",
      "type": "XML",
      "async": true
    },
    "dependencies": {
      "minUI5Version": "1.100.0",
      "libs": {
        "sap.ui.table": {},
        "sap.m": {},
        "sap.f": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "revenueltmcustom.i18n.i18n"
        }
      },
      "odata": {
        "type": "sap.ui.model.odata.v4.ODataModel",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "serviceUrl": "/analytics/"
        },
        "dataSource": "mainService"
      }
    }
  },
  "sap.app": {
    "id": "revenueltmcustom",
    "type": "application",
    "title": "Revenue LTM",
    "description": "Revenue Last 12 Months Report - Custom UI5 App",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.1.0"
    },
    "dataSources": {
      "mainService": {
        "uri": "/analytics/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": false
    }
  }
}
